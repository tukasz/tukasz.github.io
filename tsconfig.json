{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": [
      "node"
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "noUnusedLocals": true,
  },
  "include": [
    "lib/**/*.ts",
  ],
  "exclude": [
    "node_modules",
  ]
}